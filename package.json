{
  "name": "saasy",
  "version": "1.0.0",
  "main": "app.js",
  "repository": "https://github.com/pilotpirxie/saasy.git",
  "author": "pilotpirxie <10637666+pilotpirxie@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "run:js": "node ./dist/app.js",
    "run:ts": "ts-node ./src/app.ts",
    "build": "tsc && cp -r src/data dist/prisma && cp -r src/* dist",
    "dev": "SERVICE=api nodemon",
    "lint": "./node_modules/.bin/eslint src/ --ext .ts --ext .js --ext tsx --ext .jsx",
    "fix": "./node_modules/.bin/eslint --fix src/ --ext .ts --ext .js --ext tsx --ext .jsx",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
    "generate": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.4.1",
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "node-cache": "^5.1.2",
    "prisma": "^5.4.1",
    "totp-generator": "^0.0.14",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/express-serve-static-core": "^4.17.37",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/ms": "^0.7.32",
    "@types/node": "^18.18.3",
    "@types/totp-generator": "^0.0.6",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "./src/data/schema.prisma"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node ./src/app.ts"
  },
  "engines": {
    "node": "18.x"
  }
}
